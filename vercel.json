{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build:no-check",
  "outputDirectory": "client/dist",
  "installCommand": "npm install",
  "framework": "vite",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://orbitrum-connect-production.up.railway.app",
    "NODE_ENV": "production"
  }
} 